{
    "name": "xenon/laravelbdsms",
    "description": "This is package for sending sms to Bangladeshi mobile numbers using laravel application",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ariful islam",
            "email": "arif98741@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3|^7.3",
        "ext-json": "*",
        "ext-curl": "*",
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Xenon\\LaravelBDSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xenon\\LaravelBDSms\\LaravelBDSmsServiceProvider"
            ],
            "aliases": {
                "SMS": "Xenon\\LaravelBDSms\\Facades\\SMS",
                "LaravelBDSms": "Xenon\\LaravelBDSms\\Facades\\SMS"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2.0 || ^11.5",
        "orchestra/testbench": "^9.9"
    }
}
